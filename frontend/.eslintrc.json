{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "preact",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "preact"
    ],
    "rules": {
        "import/prefer-default-export": "off"
    }
}
